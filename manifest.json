{
  "name": "Wikipedia to Facebook Filler",
  "description": "Extracts data from Wikipedia and fills Facebook registration form.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting"],
  "host_permissions": [
    "https://es.wikipedia.org/*",
    "https://www.facebook.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  }
}